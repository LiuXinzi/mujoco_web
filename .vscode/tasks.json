{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake configure",
      "type": "shell",
      "command": "emcmake",
      "args": ["cmake", "-B", "build"],
      "options": {
        "cwd": "${workspaceFolder}",
        "env": {}
      },
      "group": {
        "kind": "build"
      },
      "problemMatcher": []
    },
    {
      "label": "CMake build",
      "type": "shell",
      "command": "cmake",
      "args": ["--build", "build"],
      "options": {
        "cwd": "${workspaceFolder}",
        "env": {}
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [],
      "dependsOn": ["CMake configure"]
    }
  ]
}
